pipeline {
  agent any
  stages {
    stage('develop') {
      steps {
        sh 'script'
        echo 'I want to develop'
      }
    }

    stage('Build') {
      steps {
        echo 'I want to build'
      }
    }

    stage('Test') {
      steps {
        echo 'I want to test'
      }
    }

  }
}